   "source": [
    "class Solution:\n",
    "    def sumZero(self, n: int) -> List[int]:\n",
    "        arr = []\n",
    "        for i in range(1, n//2+1):\n",
    "            arr.append(i)\n",
    "            arr.append(-i)\n",
    "        if n % 2:\n",
    "            arr.append(0)    \n",
    "        return arr"
   ]
  }
